{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["dom", "es2019"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": false,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true,
    "paths": {
      "@sequelize-ui/core/*": ["./core/*"],
      "@sequelize-ui/integration/*": ["./integration/*"],
      "@sequelize-ui/frameworks/*": ["./frameworks/*"]
    }
  },
  "exclude": ["./node_modules/", "./dist/"]
}
