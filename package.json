{
  "name": "sequelize-ui",
  "version": "0.0.3",
  "private": true,
  "license": "ISC",
  "scripts": {
    "dev": "next",
    "build": "next build && next export",
    "start": "next start",
    "lint": "eslint --cache",
    "format": "prettier --write {src,e2e}/**/*.{js,ts,tsx}",
    "check:format": "prettier --check {src,e2e}/**/*.{js,ts,tsx}",
    "check:types": "tsc -b tsconfig.check.json",
    "generate:css-types": "tailwindcss-classnames -i tailwind.config.js -o src/ui/styles/classnames/__generated__/tailwindcss-classnames.ts",
    "test": "jest",
    "test:unit": "npm test -- src",
    "test:coverage": "npm run test:unit -- --coverage",
    "test:all": "SQL_DIALECT=all npm test -- src e2e --coverage",
    "test:e2e": "jest e2e",
    "test:e2e:all": "SQL_DIALECT=all npm run test:e2e",
    "test:watch": "npm test -- --watch --"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run check:types"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.16.5",
    "@tailwindcss/forms": "^0.4.0",
    "body-scroll-lock": "^4.0.0-beta.0",
    "change-case": "^4.1.2",
    "classnames": "~2.3.1",
    "copy-to-clipboard": "^3.3.1",
    "critters": "^0.0.15",
    "fast-deep-equal": "^3.1.3",
    "file-saver": "^2.0.5",
    "focus-trap": "^6.7.1",
    "jszip": "^3.7.1",
    "next": "~12.0.7",
    "pluralize": "^8.0.0",
    "prism-react-renderer": "^1.2.1",
    "qs": "^6.10.2",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "shortid": "^2.2.16",
    "tailwindcss": "^3.0.7",
    "tailwindcss-children": "^2.1.0",
    "tailwindcss-classnames": "~3.0.0",
    "tsconfig-paths": "^3.12.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.16.5",
    "@next/bundle-analyzer": "^12.0.7",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/file-saver": "^2.0.4",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.2",
    "@types/pg": "^8.6.3",
    "@types/pluralize": "0.0.29",
    "@types/qs": "^6.9.7",
    "@types/react": "~17.0.37",
    "@types/react-dom": "~17.0.11",
    "@types/shortid": "^0.0.29",
    "@types/sqlite3": "^3.1.8",
    "@types/tedious": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "autoprefixer": "^10.4.0",
    "check-os": "^1.0.1",
    "eslint": "^8.5.0",
    "eslint-config-next": "^12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^4.3.8",
    "jest": "^27.4.5",
    "jest-localstorage-mock": "^2.4.18",
    "lint-staged": "^12.1.3",
    "mysql2": "^2.3.3",
    "nyc": "^15.1.0",
    "pg": "^8.7.1",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "react-test-renderer": "^17.0.2",
    "source-map-support": "^0.5.21",
    "sqlite3": "^4.2.0",
    "tedious": "^14.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "typescript-plugin-css-modules": "^3.4.0"
  }
}
