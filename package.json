{
  "name": "sequelize-ui",
  "version": "0.0.3",
  "private": true,
  "license": "ISC",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write .",
    "generate:css-types": "tailwindcss-classnames -i tailwind.config.js -o src/typings/__generated__/tailwindcss-classnames.d.ts",
    "test": "jest",
    "test:all": "SQL_DIALECT=all npm test -- src integration --coverage",
    "test:unit": "npm test -- src",
    "test:integration": "npm test -- integration",
    "test:integration:all": "SQL_DIALECT=all npm run test:integration",
    "test:watch": "npm run test:unit -- --coverage",
    "types:check": "tsc -b tsconfig.check.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run types:check"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@tailwindcss/forms": "^0.2.1",
    "change-case": "^4.1.2",
    "classnames": "~2.2.6",
    "copy-to-clipboard": "^3.3.1",
    "file-saver": "^2.0.5",
    "jszip": "^3.5.0",
    "next": "~10.0.8",
    "pluralize": "^8.0.0",
    "prism-react-renderer": "^1.2.0",
    "qs": "^6.5.2",
    "react": "~17.0.1",
    "react-dom": "~17.0.1",
    "shortid": "^2.2.16",
    "tailwindcss": "^2.0.3",
    "tailwindcss-children": "^2.1.0",
    "tailwindcss-classnames": "~2.0.7",
    "tsconfig-paths": "^3.9.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.13.17",
    "@types/file-saver": "^2.0.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/pg": "^7.14.9",
    "@types/pluralize": "0.0.29",
    "@types/qs": "^6.9.6",
    "@types/react": "~17.0.3",
    "@types/react-dom": "~17.0.1",
    "@types/shortid": "^0.0.29",
    "@types/sqlite3": "^3.1.7",
    "@types/tedious": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "autoprefixer": "^10.2.5",
    "check-os": "^1.0.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "mysql2": "^2.2.5",
    "nyc": "^15.1.0",
    "pg": "^8.5.1",
    "postcss": "^8.2.7",
    "prettier": "^2.2.1",
    "source-map-support": "^0.5.19",
    "sqlite3": "^5.0.1",
    "tedious": "^11.0.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
