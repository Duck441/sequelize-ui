{
  "name": "sequelize-ui",
  "version": "0.0.3",
  "private": true,
  "license": "ISC",
  "scripts": {
    "dev": "next",
    "build": "next build && next export",
    "start": "next start",
    "lint": "eslint --cache",
    "format": "prettier --write {src,e2e}/**/*.{js,ts,tsx}",
    "check:format": "prettier --check {src,e2e}/**/*.{js,ts,tsx}",
    "check:types": "tsc -b tsconfig.check.json",
    "generate:css-types": "tailwindcss-classnames -i tailwind.config.js -o src/typings/__generated__/tailwindcss-classnames.d.ts",
    "test": "jest",
    "test:unit": "npm test -- src",
    "test:coverage": "npm run test:unit -- --coverage",
    "test:all": "SQL_DIALECT=all npm test -- src e2e --coverage",
    "test:e2e": "jest e2e",
    "test:e2e:all": "SQL_DIALECT=all npm run test:e2e",
    "test:watch": "npm test -- --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run check:types"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.14.5",
    "@tailwindcss/forms": "^0.3.3",
    "change-case": "^4.1.2",
    "classnames": "~2.3.1",
    "copy-to-clipboard": "^3.3.1",
    "file-saver": "^2.0.5",
    "jszip": "^3.6.0",
    "next": "~11.0.1",
    "pluralize": "^8.0.0",
    "prism-react-renderer": "^1.2.1",
    "qs": "^6.10.1",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "shortid": "^2.2.16",
    "tailwindcss": "^2.2.2",
    "tailwindcss-children": "^2.1.0",
    "tailwindcss-classnames": "~2.1.3",
    "tsconfig-paths": "^3.9.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.14.6",
    "@next/bundle-analyzer": "^11.0.1",
    "@testing-library/react-hooks": "^7.0.0",
    "@types/file-saver": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.4",
    "@types/pg": "^8.6.0",
    "@types/pluralize": "0.0.29",
    "@types/qs": "^6.9.6",
    "@types/react": "~17.0.11",
    "@types/react-dom": "~17.0.8",
    "@types/shortid": "^0.0.29",
    "@types/sqlite3": "^3.1.7",
    "@types/tedious": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "autoprefixer": "^10.2.6",
    "check-os": "^1.0.1",
    "eslint": "^7.29.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "jest": "^27.0.5",
    "jest-localstorage-mock": "^2.4.14",
    "lint-staged": "^11.0.0",
    "mysql2": "^2.2.5",
    "nyc": "^15.1.0",
    "pg": "^8.6.0",
    "postcss": "^8.3.5",
    "prettier": "^2.3.1",
    "react-test-renderer": "^17.0.2",
    "source-map-support": "^0.5.19",
    "sqlite3": "^5.0.2",
    "tedious": "^11.0.9",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  }
}
