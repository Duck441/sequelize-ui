{
  "name": "sequelize-ui",
  "version": "0.0.3",
  "private": true,
  "license": "ISC",
  "scripts": {
    "dev": "next",
    "build": "next build && next export",
    "start": "next start",
    "format": "prettier --write .",
    "generate:css-types": "tailwindcss-classnames -i tailwind.config.js -o src/typings/__generated__/tailwindcss-classnames.d.ts",
    "test": "jest src",
    "test:coverage": "npm test -- --coverage",
    "test:all": "SQL_DIALECT=all npm test -- src e2e --coverage",
    "test:e2e": "jest e2e",
    "test:e2e:all": "SQL_DIALECT=all npm run test:e2e",
    "test:watch": "npm test -- --watch",
    "types:check": "tsc -b tsconfig.check.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run types:check"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@tailwindcss/forms": "^0.3.2",
    "change-case": "^4.1.2",
    "classnames": "~2.3.1",
    "copy-to-clipboard": "^3.3.1",
    "file-saver": "^2.0.5",
    "jszip": "^3.6.0",
    "next": "~10.1.3",
    "pluralize": "^8.0.0",
    "prism-react-renderer": "^1.2.0",
    "qs": "^6.10.1",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "shortid": "^2.2.16",
    "tailwindcss": "^2.1.2",
    "tailwindcss-children": "^2.1.0",
    "tailwindcss-classnames": "~2.1.0",
    "tsconfig-paths": "^3.9.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.13.17",
    "@next/bundle-analyzer": "^10.1.3",
    "@testing-library/react-hooks": "^5.1.2",
    "@types/file-saver": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.1",
    "@types/pg": "^7.14.11",
    "@types/pluralize": "0.0.29",
    "@types/qs": "^6.9.6",
    "@types/react": "~17.0.4",
    "@types/react-dom": "~17.0.3",
    "@types/shortid": "^0.0.29",
    "@types/sqlite3": "^3.1.7",
    "@types/tedious": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "autoprefixer": "^10.2.5",
    "check-os": "^1.0.1",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-localstorage-mock": "^2.4.10",
    "lint-staged": "^10.5.4",
    "mysql2": "^2.2.5",
    "nyc": "^15.1.0",
    "pg": "^8.6.0",
    "postcss": "^8.2.13",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.2",
    "source-map-support": "^0.5.19",
    "sqlite3": "^5.0.2",
    "tedious": "^11.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
