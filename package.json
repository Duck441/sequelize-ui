{
  "name": "sequelize-ui",
  "version": "0.0.3",
  "private": true,
  "license": "ISC",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write .",
    "debug:frameworks": "ts-node ./lib/frameworks/debug.ts",
    "generate:css-types": "tailwindcss-classnames -i tailwind.config.js -o typings/__generated__/tailwindcss-classnames.d.ts",
    "test:unit": "npm run test:ts -- 'lib/**/*.spec.ts' --watch ",
    "test:sql": "npm run test:ts -- 'tests/integration/sql.spec.ts' --exit",
    "test:ts": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha",
    "type:check": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run type:check"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.2.1",
    "change-case": "^4.1.2",
    "classnames": "~2.2.6",
    "file-saver": "^2.0.5",
    "jszip": "^3.5.0",
    "next": "~10.0.8",
    "pluralize": "^8.0.0",
    "prism-react-renderer": "^1.2.0",
    "qs": "^6.5.2",
    "react": "~17.0.1",
    "react-dom": "~17.0.1",
    "tailwindcss": "^2.0.3",
    "tailwindcss-children": "^2.1.0",
    "tailwindcss-classnames": "~2.0.7",
    "tsconfig-paths": "^3.9.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/file-saver": "^2.0.1",
    "@types/mocha": "^8.2.0",
    "@types/mocha-each": "^2.0.0",
    "@types/node": "^14.14.22",
    "@types/pg": "^7.14.9",
    "@types/pluralize": "0.0.29",
    "@types/qs": "^6.9.6",
    "@types/react": "~17.0.3",
    "@types/react-dom": "~17.0.1",
    "@types/sqlite3": "^3.1.7",
    "@types/tedious": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "autoprefixer": "^10.2.5",
    "chai": "^4.3.0",
    "check-os": "^1.0.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "mocha": "^8.2.1",
    "mocha-each": "^2.0.1",
    "mysql2": "^2.2.5",
    "pg": "^8.5.1",
    "postcss": "^8.2.7",
    "prettier": "^2.2.1",
    "sqlite3": "^5.0.1",
    "tedious": "^11.0.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
